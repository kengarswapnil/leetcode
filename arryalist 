import java.util.ArrayList;
 
class Main {
  public static void main(String[] args){
 
    // create an ArrayList
    ArrayList<String> Shapes = new ArrayList<>();
 
    // Add elements to ArrayList
    Shapes.add("Rectangle");
    Shapes.add("Circle");
    Shapes.add("Triangle");
    Shapes.add("Square");
    System.out.println("ArrayList: " +Shapes);
  }
}
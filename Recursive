public class Recursive {

   public static int Fibo(int n)
   {
          if(n<=1)
          {
            return 1;
          }
          else
          {
            return Fibo(n-1)+Fibo(n-2);
          }
   }

    public static void main( String  args[])
    {
       
 
       int res =Fibo(4);
       System.out.println(res);


    }
}
